            "image-rendering",
            "inline-size",
            "min-inline-size",
            "max-inline-size",
            "block-size",
            "min-block-size",
            "max-block-size",
            "table-layout",
            "caption-side",
            "empty-cells",
            "resize",
            "object-fit",
            "object-position",
            "scroll-behavior",
